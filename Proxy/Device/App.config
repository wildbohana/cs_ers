<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<system.serviceModel>
		<client>
			<endpoint name="Server"
                      address="net.tcp://localhost:8001/Server"
                      binding="netTcpBinding"
                      contract="Common.Interfejsi.IServer" />
		</client>
	</system.serviceModel>
	
	<!-- Konfiguracija za vreme -->
	<appSettings>
		<add key="slanjeSati" value="0"/>
		<add key="slanjeMinute" value="5"/>
		<add key="slanjeSekunde" value="0"/>
	</appSettings>
</configuration>