<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<system.serviceModel>
		<services>
			<service name="Proxy.ProksiServis">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:8002" />
					</baseAddresses>
				</host>
				<endpoint address="Proksi"
                          binding="netTcpBinding"
                          contract="Common.Interfejsi.IProksi" />
			</service>
		</services>
	</system.serviceModel>

	<!-- Konfiguracija za vreme -->
	<appSettings>
		<add key="vremeProvera" value="5"/>
		<add key="vremeZastareli" value="24"/>
	</appSettings>
</configuration>