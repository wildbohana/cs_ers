<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<!-- WCF -->
	<system.serviceModel>
		<services>
			<service name="Proxy.ProksiServis">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:8002" />
					</baseAddresses>
				</host>
				<endpoint address="Proksi"
                          binding="netTcpBinding"
                          contract="Common.Interfejsi.IProksi" />
			</service>
		</services>
	</system.serviceModel>

	<!-- TODO izmeni kako treba -->	
	<!-- Konfiguracija za vreme -->
	<appSettings>
		<!-- 5 minuta - između dve provere -->
		<add key="proveraSati" value="0"/>
		<add key="proveraMinute" value="0"/>
		<add key="proveraSekunde" value="10"/>

		<!-- 24 sata - podatku se ne pristupa da bi se obrisao -->
		<add key="brisanjeSati" value="0"/>
		<add key="brisanjeMinute" value="0"/>
		<add key="brisanjeSekunde" value="15"/>
	</appSettings>
</configuration>